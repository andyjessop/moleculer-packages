{
  "root": "packages/moleculer-universal-api",
  "sourceRoot": "packages/moleculer-universal-api/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/node:package",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/moleculer-universal-api",
        "tsConfig": "packages/moleculer-universal-api/tsconfig.lib.json",
        "packageJson": "packages/moleculer-universal-api/package.json",
        "main": "packages/moleculer-universal-api/src/index.ts",
        "assets": ["packages/moleculer-universal-api/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/moleculer-universal-api/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/moleculer-universal-api"],
      "options": {
        "jestConfig": "packages/moleculer-universal-api/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
